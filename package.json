{
  "name": "iban-ts",
  "version": "0.11.0",
  "description": "A TypeScript library for validating, formatting, and converting IBAN (International Bank Account Number) and BBAN (Basic Bank Account Number), offering comprehensive support for international banking data standards.",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/iban-ts.esm.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/iban-ts.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint src",
    "prepare": "husky",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "peerDependencies": {},
  "author": "Altrim Beqiri",
  "repository": {
    "type": "git",
    "url": "https://github.com/altrim/iban.js.git"
  },
  "size-limit": [
    {
      "path": "dist/iban-ts.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/iban-ts.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.2.0",
    "@tsconfig/recommended": "1.0.13",
    "dts-cli": "2.0.5",
    "husky": "9.1.7",
    "size-limit": "11.2.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  }
}
