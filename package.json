{
  "name": "iban-ts",
  "version": "0.2.0",
  "description": "A javascript library to validate, format and convert IBAN (International Bank Account Number) and BBAN (Basic Bank Account Number)",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/iban-ts.esm.js",
  "exports": {
    ".": {
      "import": "./dist/iban-ts.esm.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Laurent VB",
  "repository": "https://github.com/altrim/iban.js",
  "size-limit": [
    {
      "path": "dist/iban.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/iban.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@tsconfig/recommended": "1.0.1",
    "dts-cli": "1.2.0",
    "size-limit": "7.0.8",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  }
}
